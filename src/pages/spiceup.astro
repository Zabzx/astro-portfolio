---
import Layout from "../layouts/Layout.astro";
import '@fontsource/dela-gothic-one';
import "@fontsource-variable/raleway";
import SpiceUp from "../../public/img/spiceup.png";
---

<Layout>
    <section class="bg-mainBg bg-center bg-cover sm:h-screen">
        <div class="container px-16">
        <div class="flex flex-col sm:flex-row justify-between">
            <div class="project sm:w-1/2">
                <div class="bg-fadedBlack sm:px-28 py-10 rounded-bl-xl rounded-br-xl">
                    <h1 class="text-white text-3xl font-sans text-center">SpiceUp!</h1>
                </div>

                <div class="">
                    <img src={SpiceUp.src} class="w-project mt-10" alt="" transition:name="spiceup">
                    <p class="font-raleway text-white mt-5">SpiceUp! is an international food app that allows users to discover and learn to prepare dishes from all over the world. communicating with a REST API, this app offers a user friendly UX that allows users to quickly navigate their saved dishes for ease of access.</p>
                    <div class="flex gap-5">
                        <a href="https://github.com/Zabzx/spiceUp" target="_blank">
                        <button class="bg-fadedWhite p-3 rounded-md border-white border-4 text-white mt-5">Source Code</button>
                        </a>

                        <a href="https://spicedup.netlify.app/" target="_blank">
                        <button class="bg-spiceRed p-3 rounded-md border-white border-4 text-white mt-5">Live App</button>
                        </a>
                    </div>
                </div>
            </div>
            <div class="stack w-1/2 flex flex-col items-center">
